function dtxt()
	if _s.atxt!=nil then
		dr(0+_s.cx,108+_s.cy,128,24,0)
		print("\n".._s.atxt,5+_s.cx,108+_s.cy,7)
		print("\n\n"..txts.f[_s.txtt],5+_s.cx,108+_s.cy,10)
		_s.atxt=nil
	end
end

--inventory
function openinv(pinv)
	_s.msts=false
	
	_s.invb=pinv
	_s.invi=1
	
	_s.ftt=0.08
	setfade("out",function()
		_s.dsts=false
		_s.inv=true
		setfade("in")
	end)
--	_s.dsts
end

function closeinv()
	setfade("out",function()
		_s.dsts=true
		_s.inv=false
		_s.msts=true
		setfade("in")
	end)
end

function dinv()
	if _s.inv then
--		dr(32,20,64,88,0)
