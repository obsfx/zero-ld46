--camera
function ucam(x,y)
	_s.cc=flr(x/128)*16
	_s.cr=flr(y/128)*16
	_s.cx=flr(x/128)*128
	_s.cy=flr(y/128)*128
	printh(_s.cc.." ".._s.cr)
	camera(_s.cx,_s.cy)
end

--room
function enterroom(x,y)
	_s.ftt=0.08
	_s.msts=false
	
	_s.rmid="r"..x.."_"..y
	
	_s.oldpx=_s.plyr.x
	_s.oldpy=_s.plyr.y
	
	_s.rmcx=rms[_s.rmid].cx
	_s.rmcy=rms[_s.rmid].cy
	_s.rmcw=rms[_s.rmid].cw
	_s.rmch=rms[_s.rmid].ch
	
	setfade("out",function()
		_s.msts=true
		_s.rmmode=true
		_s.plyr.x=rms[_s.rmid].px
		_s.plyr.y=rms[_s.rmid].py
		setfade("in")
	end)
end

function quitroom()
	_s.msts=true
	
	setfade("out",function()
		_s.msts=true
		_s.rmmode=false
		_s.plyr.x=_s.oldpx
		_s.plyr.y=_s.oldpy
		setfade("in")
	end)
end
