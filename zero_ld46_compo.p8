--objects
function rectangle(x,y,w,h)
	return {
		x=x,
		y=y,
		w=w,
		h=h,
		
		--get left
		get_l=function(self)
			return self.x
		end,
		
		--get right
		get_r=function(self)
			return self.x+self.w
		end,
		
		--get top
		get_t=function(self)
			return self.y
		end,
		
		--get bottom
		get_b=function(self)
			return self.y+self.h
		end,
		
		--half width
		get_hw=function(self)
			return self.w*0.5
		end,
	
		--half height
		get_hh=function(self)
			return self.h*0.5
		end,
		
		--midx
		get_mx=function(self)
			return self.x+self:get_hw()
		end,
		
		--midy
		get_my=function(self)
			return self.y+self:get_hh()
		end
	}
end

function obj(x,y,w,h,de)
	local t={
		de=de or false,
		col=de or false,
		ovlp=de or false,
	}
	
	return merge_t(t,rectangle(x,y,w,h))
end
