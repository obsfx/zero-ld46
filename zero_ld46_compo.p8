--zero
function dr(x,y,w,h,c)
	rectfill(x,y,x+w-1,y+h-1,c)
end

function drb(x,y,w,h,c)
	rect(x,y,x+w-1,y+h-1,c)
end

function merge_t(t1,t2)
	for k,v in pairs(t2) do 
		t1[k] = v 
	end
	
	return t1
end

function merge_arr(a,b)
	for bv in all(b) do
		add(a, bv)
	end
	
	return a
end
