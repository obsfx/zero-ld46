--collision
col={
	check=function(self,a,b,incb)
			local l1=a:get_l()
			local r1=a:get_r()
			local t1=a:get_t()
			local b1=a:get_b()
			
			local l2=b:get_l()
			local r2=b:get_r()
			local t2=b:get_t()
			local b2=b:get_b()
			
			local midx1=a:get_mx()
			local midy1=a:get_my()
			
			local midx2=b:get_mx()
			local midy2=b:get_my()
			
			local cond=r1<l2 or l1>r2 or b1<t2 or t1>b2
			
			if incb then 
				cond=r1<=l2 or l1>=r2 or b1<=t2 or t1>=b2	
			end
			
			return not cond
	end,
}
